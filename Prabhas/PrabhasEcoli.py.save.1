


x = open("GCA_000005845.2_ASM584v2_genomic.fna", "r")
x.readline()
y= x.read()

y= y[189:255]
print(y)

proteins = {"Alanine" : 0, "Arginine" : 0, "Asparagine" : 0, "Aspartic Acid" : 0,
            "Cysteine" : 0, "Glutamic Acid" : 0, "Glutamine" : 0, "Glycine" : 0,
